<h1>Articles Listing Page</h1>
<table> 
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Actions</th>
    </tr>
  </thead>
<tbody>
  <% @articles.each do |var| %>
    <tr>
      <td><%= var.title %></td>
      <td><%= var.description %></td>
      <td>Some title</td>
    </tr>
  <% end %>
</tbody>
</table>